  <!--
    For a description of web.config changes for .NET 4.5 see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5">
      <assemblies>
        <add assembly="System.Web.Abstractions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Helpers, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Routing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Mvc, Version=3.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.WebPages, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
      </assemblies>
    </compilation>
    <pages controlRenderingCompatibilityVersion="4.0">
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
        <add namespace="Microsoft.Web.Mvc" />
        <add namespace="Profiling2.Web.Mvc.Controllers" />
      </namespaces>
    </pages>
    <authentication mode="Forms">
      <forms name="Profiling2" loginUrl="~/Account/Login" protection="All" path="/" timeout="180" slidingExpiration="true" />
    </authentication>
    <membership defaultProvider="LocalMembershipProvider">
      <providers>
        <clear />
        <add name="LocalMembershipProvider" type="Profiling2.Infrastructure.Security.LocalMembershipProvider" />
        <add name="SetupMembershipProvider" type="Profiling2.Infrastructure.Security.SetupMembershipProvider" />
        <add name="ActiveDirectoryMembershipProvider" type="Profiling2.Infrastructure.Security.ActiveDirectoryMembershipProvider" />
      </providers>
    </membership>
    <roleManager defaultProvider="LocalRoleProvider" enabled="true">
      <providers>
        <clear />
        <add name="LocalRoleProvider" type="Profiling2.Infrastructure.Security.LocalRoleProvider" />
      </providers>
    </roleManager>
    <httpRuntime executionTimeout="600" maxRequestLength="51200" />
    
    
  <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" />
    </httpModules><webServices>
      <soapExtensionTypes>
        <add type="HrdbWebServiceClient.Infrastructure.XmlCleanupSoapExtension, HrdbWebServiceClient" priority="1" group="0" />
      </soapExtensionTypes>
    </webServices></system.web>
  <system.webServer>
    <!-- GZip static file content.  Overrides the server default which only compresses static files over 2700 bytes -->
    <httpCompression directory="%SystemDrive%\websites\_compressed" minFileSizeForComp="1024">
      <scheme name="gzip" dll="%Windir%\system32\inetsrv\gzip.dll" />
      <staticTypes>
        <add mimeType="text/*" enabled="true" />
        <add mimeType="message/*" enabled="true" />
        <add mimeType="application/javascript" enabled="true" />
        <add mimeType="application/x-javascript" enabled="true" />
        <add mimeType="application/json" enabled="true" />
        <add mimeType="*/*" enabled="false" />
      </staticTypes>
    </httpCompression>
    <modules runAllManagedModulesForAllRequests="true"><add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" /><add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" preCondition="managedHandler" /><add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" preCondition="managedHandler" /></modules>
    <handlers />
    <httpErrors existingResponse="PassThrough"></httpErrors>
  <validation validateIntegratedModeConfiguration="false" /></system.webServer>
