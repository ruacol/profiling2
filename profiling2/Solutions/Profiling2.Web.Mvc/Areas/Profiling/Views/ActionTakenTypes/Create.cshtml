@using Profiling2.Web.Mvc.Areas.Profiling.Controllers.ViewModels
@model ActionTakenTypeViewModel
@{
  ViewBag.Title = "Create Action Taken Type";
}

<h2>Create Action Taken Type</h2>

<hr />

@using (Html.BeginForm("Create", "ActionTakenTypes", FormMethod.Post, new { @class = "form-horizontal" }))
{
    @Html.Partial("_ActionTakenTypeForm")
    
    <div class="form-actions">
        <button type="submit" class="btn btn-primary">Save</button>
        @Html.ActionLink("Back", "Index", "ActionTakenTypes", new { area = "Profiling" }, new { @class = "btn" })
    </div>
}
