@using Profiling2.Web.Mvc.Areas.Profiling.Controllers.ViewModels
@model RankViewModel
@{
    ViewBag.Title = "Create Rank";
}

<h2>Create Rank</h2>

<hr />

@using (Html.BeginForm("Create", "Ranks", FormMethod.Post, new { @class = "form-horizontal" }))
{
    @Html.Partial("_RankForm", Model)
    
    <div class="form-actions">
        <button type="submit" class="btn btn-primary">Save</button>
        @Html.ActionLink("Back", "Index", "Ranks", new { area = "Profiling" }, new { @class = "btn" })
    </div>
}
