@using Profiling2.Web.Mvc.Areas.Profiling.Controllers.ViewModels
@model RegionViewModel
@{
    ViewBag.Title = "Create Pre-2015 Province";
}

<h2>Create Pre-2015 Province</h2>

<hr />

@using (Html.BeginForm("Create", "Regions", FormMethod.Post, new { @class = "form-horizontal" }))
{
    @Html.Partial("_RegionForm", Model)
    
    <div class="form-actions">
        <button type="submit" class="btn btn-primary">Save</button>
        @Html.ActionLink("Back", "Index", "Regions", new { area = "Profiling" }, new { @class = "btn" })
    </div>
}
